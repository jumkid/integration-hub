  "bugs": {
    "url": "https://github.com/jumkid/integration-hub/issues"
  },
  "homepage": "https://github.com/jumkid/integration-hub#readme",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "express": "^4.17.1",
    "rimraf": "^3.0.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0"
  }
